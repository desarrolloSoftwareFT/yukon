! **************************************************************************

unit "YA vs A0"
 execute A0_YA_0
 execute A0_YA_1
 execute A0_YA_Z
end unit

unit "YA vs A1"
 execute A1_YA_0
 execute A1_YA_1
 execute A1_YA_Z
end unit

unit "YB vs B0"
 execute B0_YB_0
 execute B0_YB_1
 execute B0_YB_Z
end unit

unit "YB vs B1"
