end vector

vector B1_YB_Z
 set OEB to "1"
 set SEL to "1"
 set B1  to "0"
 set YB  to "1"
end vector

vector C0_YC_0
 set OEB to "0"
 set SEL to "0"
 set C0 to "0"
 set YC  to "0"
end vector

vector C0_YC_1
 set OEB to "0"
 set SEL to "0"
